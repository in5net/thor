{
  "name": "@limitlesspc/thor",
  "version": "1.0.3",
  "author": {
    "name": "Izaak Brown",
    "email": "izaakbthunderbird@gmail.com"
  },
  "repository": "https://github.com:limitlesspc/thor.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "dist"
  ],
  "module": "./dist/thor.es.js",
  "main": "./dist/thor.umd.js",
  "types": "./dist/thor.d.ts",
  "exports": {
    ".": {
      "import": "./dist/thor.es.js",
      "require": "./dist/thor.umd.js"
    }
  },
  "scripts": {
    "exec": "ts-node main.ts",
    "dev": "vite",
    "build": "run-s build:*",
    "build:vite": "vite build",
    "build:types": "tsup src/thor.ts --dts && rm dist/thor.js",
    "lint": "eslint --ignore-path .gitignore .",
    "pub": "run-s build && npm publish"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "readline-sync": "^1.4.10"
  },
  "devDependencies": {
    "@types/node": "^16.6.2",
    "@types/readline-sync": "^1.4.4",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.1",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "tsup": "^4.14.0",
    "typescript": "^4.3.5",
    "vite": "^2.5.0"
  }
}
